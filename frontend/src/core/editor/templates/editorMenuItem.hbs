<div class="editor-menu-item-inner">
	<div class="editor-menu-item-title">
		<h4>{{{title}}}</h4>
	</div>
	<div class="editor-menu-item-buttons">
		{{#if_value_equals _type 'menu'}}
		<button class="editor-menu-item-view">View Children</button>
		{{/if_value_equals}}
		{{#if_value_equals _type 'page'}}
		<button class="editor-menu-item-view">View</button>
		{{/if_value_equals}}
		<button class="editor-menu-item-edit">Edit</button>
		{{#if_value_equals _type 'course'}}
		{{else}}
		<button class="editor-menu-item-delete">Delete</button>
		{{/if_value_equals}}
	</div>
</div>