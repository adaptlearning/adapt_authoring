<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- TODO needs to be localisable -->
    <title>Adapt Authoring Tool</title>
    <link rel="icon" type="image/png" href="/adaptbuilder/css/assets/favicon.png" />
    <link rel="stylesheet" href="/adaptbuilder/css/adapt.css" />
    <link rel="stylesheet" href="/adaptbuilder/css/assets/sweetalert.css" />
    <link rel="stylesheet" href="core/libraries/font-awesome-4.2.0/css/font-awesome.min.css">
    <script type="text/javascript" src="core/libraries/modernizr.js"></script>
    <script src="//cdn.ckeditor.com/4.5.4/full-all/ckeditor.js"></script>
    {{#if isProduction}}
    <script type="text/javascript" src="core/libraries/require.js" data-main="adaptbuilder/js/origin.js{{#if dateStampAsString}}?{{dateStampAsString}}{{/if}}"></script>
    {{else}}
    <script type="text/javascript" src="core/libraries/require.js"></script>
    <script type="text/javascript" src="core/app/config.js"></script>
    <script type="text/javascript" src="core/app/app.js"></script>
    {{/if}}
    <meta http-equiv="cache-control" content="no-store, no-cache, must-revalidate" />
    <meta http-equiv="Pragma" content="no-store, no-cache" />
    <meta http-equiv="Expires" content="0" />
  </head>
  <body>
    <div id="app" class="app container">
      <div class="location-title">
      </div>
      <div class="app-inner">
      </div>
      <div class="loading">
        <div class="loading-inner">
          <div class="loading-image"><h3>Loading...</h3></div>
        </div>
      </div>
    </div>
  </body>
</html>
